<?xml version="1.0" encoding="UTF-8" ?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="com.cordova.plugin.samsung.pass" version="1.0.0">
    <name>SamsungPass</name>
    <description>Implements Fingerprint Scanning with Samsung devices.</description>
    <author>Trevor Brindle (https://github.com/tabrindle)</author>
    <repo>https://github.com/tabrindle/cordova-plugin-samsungpass.git</repo>
    <platform name="android">
        <js-module src="www/SamsungPass.js" name="SamsungPass">
            <clobbers target="SamsungPass" />
        </js-module>
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="SamsungPass">
                <param name="android-package" value="com.cordova.plugin.samsung.pass" />
            </feature>
        </config-file>
        <source-file src="src/android/SamsungPassPlugin.java" target-dir="src/com/cordova/plugin/samsung/pass/SamsungPassPlugin.java" />
        <source-file src="src/android/pass-v1.1.3.jar" target-dir="libs/pass-v1.1.3.jar" />
        <source-file src="src/android/sdk-v1.0.0.jar" target-dir="libs/sdk-v1.0.0.jar" />
    </platform>
</plugin>
